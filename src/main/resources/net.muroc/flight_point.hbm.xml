<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="net.muroc.FLTrackPoint" table="adsb_flight_points">
        <meta attribute="class-description">
            This class contains the employee detail.
        </meta>
        <id name="ID" type="int" column="ID">
            <generator class="assigned"/>
        </id>
        <property name="hex" column="hex" type="string"/>
        <property name="squawk" column="squawk" type="string"></property>
        <property name="flight" column="flight" type="string"></property>
        <property name="lat" column="lat" type="double"/>
        <property name="lon" column="lon" type="double"/>
        <property name="nucp" column="nucp" type="double"></property>
        <property name="seen_pos" column="seen_pos" type="double"/>
        <property name="altitude" column="altitude" type="integer"/>
        <property name="vert_rate" column="vert_rate" type="integer"/>
        <property name="track" column="track" type="integer"/>
        <property name="speed" column="speed" type="integer"/>
        <property name="messages" column="messages" type="integer"/>
        <property name="seen" column="seen" type="double"/>
        <property name="rssi" column="rssi" type="double"></property>
    </class>
</hibernate-mapping>